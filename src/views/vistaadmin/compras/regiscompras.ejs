                            if (celdaCategoria) {
                                var textoCategoria = celdaCategoria.textContent || celdaCategoria.innerText;

                                // Compara el texto de la celda con el filtro
                                if (textoCategoria.toUpperCase().indexOf(filtro) > -1) {
