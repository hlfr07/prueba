                <script>
                    var listaProductos = [];

                    function agregarProducto(idproducto, nombre, categoria, descripcion, unidad) {
                        var producto = {
                            idproducto: idproducto,
                            nombre: nombre,
                            categoria: categoria,
                            unidad: unidad,
                            precio: 0,
                            descripcion: descripcion,
                            cantidad: 0,
                            subtotal: 0
                        };

                        listaProductos.push(producto);

                        actualizarTabla();
                        actualizarTotal();
                        actualizarTotal1();

                    }

                    async function enviarLista() {
                        try {
                            const fecha = document.getElementById('idfecha').value;
                            const hora = document.getElementById('idhora').value;
                            const ruc = document.getElementById('ruc').value;
                            const serie = document.getElementById('idserie').value;
                            const correlativo = document.getElementById('idcorrelativo').value;
                            const subtotal = document.getElementById('idtotal').value;
                            const igv = document.getElementById('idigv').value;
                            const total = document.getElementById('idtotal1').value;
                            const idproveedor = document.getElementById('idproveedor').value;
                            const idusuario = document.getElementById('idusuario').value;

                            const compra = {
                                fecha: fecha,
                                hora: hora,
                                ruc: ruc,
                                serie: serie,
                                correlativo: correlativo,
                                subtotal: subtotal,
                                igv: igv,
                                total: total,
                                idproveedor: idproveedor,
                                idusuario: idusuario,
                            };

                            console.log(compra);

                            // 1. Realizar una solicitud de tipo DELETE con axios
                            const deleteResponse = await axios.patch('/admin/sumarcantidad', { productos: listaProductos });
                            console.log('DELETE response:', deleteResponse.data);

                            // 2. Realizar una solicitud de tipo POST con axios para registrar la compra
                            const primerPostResponse = await axios.post('/admin/regcompras', compra);
                            console.log('Primer POST response:', primerPostResponse.data);

                            // Obtener el ID de la compra reci√©n registrada
                            const idCompra = primerPostResponse.data.id_compra;

                            // 3. Actualizar el ID de compra en cada producto de la lista
                            listaProductos.forEach(producto => {
                                producto.id_compra = idCompra;
                            });

                            // 4. Realizar otra solicitud de tipo POST con axios para registrar la lista de productos
                            const segundoPostResponse = await axios.post('/admin/regcompras2', { productos: listaProductos });
                            console.log('Segundo POST response:', segundoPostResponse.data);

                            Swal.fire({
                                icon: 'success',
                                title: 'Registro Exitoso',
                                text: 'La Compra se realiz√≥ correctamente.',
                                timer: 2000,
                                showConfirmButton: false
                            });

                            setTimeout(function () {
                                window.location.href = '/admin/regcompras';
                            }, 2000);
                        } catch (error) {
                            console.error(error);

                            Swal.fire({
                                icon: 'error',
                                title: 'Error',
                                text: 'ALGO SALI√ì MAL CONSULTE ADMIN',
                                timer: 2000,
                                showConfirmButton: false
                            });
                        }
                    }

                    function actualizarTabla() {
                        var cuerpoTablaProductos = document.getElementById("cuerpoTablaProductos");
                        cuerpoTablaProductos.innerHTML = "";

                        listaProductos.forEach(producto => {
                            var fila = cuerpoTablaProductos.insertRow();

                            var celdaNombre = fila.insertCell(0);
                            var celdaCategoria = fila.insertCell(1);
                            var celdaDescripcion = fila.insertCell(2);
                            var celdaUnidad = fila.insertCell(3);
                            var celdaPrecio = fila.insertCell(4);
                            var celdaCantidad = fila.insertCell(5);
                            var celdaSubtotal = fila.insertCell(6);
                            var celdaAcciones = fila.insertCell(7);

                            celdaNombre.textContent = producto.nombre;
                            celdaCategoria.textContent = producto.categoria;
                            celdaDescripcion.textContent = producto.descripcion;
                            celdaUnidad.textContent = producto.unidad;

                            var inputPrecio = document.createElement("input");
                            inputPrecio.type = "text";
                            inputPrecio.value = producto.precio;
                            inputPrecio.addEventListener("input", function () {
                                this.value = this.value.replace(/[^0-9.]/g, '');
                                producto.precio = parseFloat(this.value) || 0;
                                producto.subtotal = producto.precio * producto.cantidad;
                                celdaSubtotal.textContent = producto.subtotal.toFixed(2);
                                actualizarTotal();
                                actualizarTotal1();
                            });
                            celdaPrecio.appendChild(inputPrecio);

                            var inputCantidad = document.createElement("input");
                            inputCantidad.type = "text";
                            inputCantidad.value = producto.cantidad;
                            inputCantidad.addEventListener("input", function () {
                                this.value = this.value.replace(/[^0-9]/g, '');
                                producto.cantidad = parseInt(this.value) || 0;
                                producto.subtotal = producto.precio * producto.cantidad;
                                celdaSubtotal.textContent = producto.subtotal.toFixed(2);
                                actualizarTotal();
                                actualizarTotal1();
                            });
                            celdaCantidad.appendChild(inputCantidad);

                            celdaSubtotal.textContent = producto.subtotal.toFixed(2);

                            var botonQuitar = document.createElement("button");
                            botonQuitar.textContent = "Quitar";
                            botonQuitar.onclick = function () {
                                listaProductos.splice(listaProductos.indexOf(producto), 1);
                                actualizarTabla();
                                actualizarTotal();
                                actualizarTotal1();
                            };
                            celdaAcciones.appendChild(botonQuitar);

                        });
                        console.log(listaProductos);
                    }

                    function actualizarTotal() {
                        var total = listaProductos.reduce((sum, producto) => sum + producto.subtotal, 0);
                        document.getElementById("idtotal").value = total.toFixed(2);
                        actualizarTotal1(); // Actualizar el total1 cuando cambie el total
                    }

                    function actualizarTotal1() {
                        var total1 = parseFloat(document.getElementById("idtotal").value) || 0;
                        var igv = parseFloat(document.getElementById("idigv").value) || 0;
                        var total2 = (total1 + total1 * igv);
                        document.getElementById("idtotal1").value = total2.toFixed(2);
                    }

                    // A√±ade este bloque al final de tu script
                    document.getElementById("idigv").addEventListener("input", function () {
                        // Eliminar caracteres no permitidos
                        this.value = this.value.replace(/[^0-9.]/g, '');
                        actualizarTotal1(); // Actualizar el total1 cuando cambie el IGV
                    });

                    async function Selecionar(idProveedor, ruc, nombreProveedor, idcategoria) {
                        const apiUrl = `https://worldtimeapi.org/api/timezone/America/Lima`;

                        try {
                            const response = await fetch(apiUrl);
                            const data = await response.json();

                            const fecha = data.datetime.split('T')[0]; // Obtiene la fecha (formato 'YYYY-MM-DD')
                            const hora = data.datetime.split('T')[1].slice(0, 5); // Obtiene la hora (formato 'HH:MM')

                            // Muestra la fecha en el campo de entrada con ID 'idfecha' y la hora en 'idhora'
                            document.getElementById('idfecha').value = fecha;
                            document.getElementById('idhora').value = hora;
                        } catch (error) {
                            console.log("Error al obtener la fecha y hora desde la API:", error);
                        }

                        // Resto del c√≥digo para asignar valores a otros campos
                        document.getElementById('ruc').value = ruc;
                        document.getElementById('nombreproveedor').value = nombreProveedor;
                        document.getElementById('idproveedor').value = idProveedor;
                        document.getElementById('idusuario').value = '<%= idusuario %>';
                        document.getElementById('idcategoria').value = idcategoria;

                        // Aqu√≠ puedes hacer lo que necesites con idProveedor, ruc y nombreProveedor
                        console.log('Proveedor seleccionado - ID:', document.getElementById('idproveedor').value);
                        console.log('Usuario ejecutor - ID:', document.getElementById('idusuario').value);
                        console.log('Proveedor seleccionado - RUC:', ruc);
                        console.log('Proveedor seleccionado - Nombre:', nombreProveedor);
                        console.log('Proveedor seleccionado - Nombre:', document.getElementById('idcategoria').value);

                        // Tambi√©n puedes enviar idProveedor, ruc y nombreProveedor al servidor mediante una petici√≥n AJAX si es necesario
                        
                    }
                    //IGV 
                    // Obtener el campo de entrada por su ID
                    const inputIGV = document.getElementById('idigv');






