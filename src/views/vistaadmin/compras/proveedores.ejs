                <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
                <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.all.min.js"></script>


                <script>


                    function postDatos() {
                        // Obtener los valores de los campos de entrada
                        const ruc = document.getElementById('ruc').value;
                        const nombre_empresa = document.getElementById('nombre_empresa').value;
                        const direccion = document.getElementById('direccion').value;
                        const ciudad = document.getElementById('ciudad').value;
                        const pais = document.getElementById('pais').value;
                        const telefono = document.getElementById('telefono').value;
                        const email = document.getElementById('email').value;
                        const nombre_categoria = document.getElementById('nombre_categoria').value;

                        // Crear un objeto de datos con los valores para usar en los controolers
                        const data = {
                            ruc: ruc,
                            nombre_empresa: nombre_empresa,
                            direccion: direccion,
                            ciudad: ciudad,
                            pais: pais,
                            telefono: telefono,
                            email: email,
                            nombre_categoria: nombre_categoria,
                        };

                        // Enviar una solicitud POST a la ruta "clientes" utilizando Axios
                        axios.post('/admin/proveedores', data)
                            .then(function (response) {
                                // Mostrar un mensaje de √©xito con SweetAlert
                                Swal.fire({
                                    icon: 'success',
                                    title: 'Registro exitoso',
                                    text: 'El Proveedor se realiz√≥ correctamente.',
                                    timer: 2000, // Cambia la duraci√≥n del mensaje si lo deseas
                                    showConfirmButton: false // Oculta el bot√≥n "OK"
                                });

                                // Redirigir al usuario despu√©s de 2 segundos (2000 ms)
                                setTimeout(function () {
                                    window.location.href = '/admin/proveedores';
                                }, 2000);
                            })

                    }


                    function botoneditar(idproveedor) {
                        // Asignar el ID del perfil al campo oculto

                        axios.get(`/admin/proveedores/${idproveedor}`)
                            .then(function (response) {
                                const respuestadelquery = response.data;
                                if (respuestadelquery) {
                                    console.log(respuestadelquery); // Imprimir los datos en la consola
                                    document.getElementById('idproveedor').value = respuestadelquery.idproveedor;
                                    document.getElementById('nombre_empresaimput').value = respuestadelquery.nombre_empresa;
                                    document.getElementById('ruc1').value = respuestadelquery.ruc;
                                    document.getElementById('direccion1').value = respuestadelquery.direccion;
                                    document.getElementById('ciudad1').value = respuestadelquery.ciudad;
                                    document.getElementById('pais1').value = respuestadelquery.pais;
                                    document.getElementById('telefono1').value = respuestadelquery.telefono;
                                    document.getElementById('email1').value = respuestadelquery.email;
                                    document.getElementById('nombre_categoria1').value = respuestadelquery.idcategoria;
                                }
                            })
                            .catch(function (error) {
                                console.error(error);
                            });
                    }


                    function guardarDatos() {
                        // Obt√©n el valor del campo de entrada
                        // Obtener los valores de los campos de entrada
                        const idproveedor = document.getElementById('idproveedor').value;
                        const nombre_empresa1 = document.getElementById('nombre_empresaimput').value;
                        const direccion1 = document.getElementById('direccion1').value;
                        const ciudad1 = document.getElementById('ciudad1').value;
                        const pais1 = document.getElementById('pais1').value;
                        const telefono1 = document.getElementById('telefono1').value;
                        const email1 = document.getElementById('email1').value;
                        const nombre_categoria1 = document.getElementById('nombre_categoria1').value;

                        // Crear un objeto de datos con los valores para usar en los controolers
                        const data = {
                            nombre_empresa1: nombre_empresa1,
                            direccion1: direccion1,
                            ciudad1: ciudad1,
                            pais1: pais1,
                            telefono1: telefono1,
                            email1: email1,
                            nombre_categoria1: nombre_categoria1,
                        };

                        console.log(data);


                        axios.put(`/admin/proveedores/${idproveedor}`, data)
                            .then(function (response) {
                                Swal.fire({
                                    icon: 'success',
                                    title: 'Registro exitoso',
                                    text: 'El Proveedor se realiz√≥ correctamente.',
                                    timer: 2000,
                                    showConfirmButton: false
                                }).then(function () {
                                    // Despu√©s de que el usuario haya visto la notificaci√≥n, actualiza la p√°gina
                                    location.reload(); // Esto recargar√° la p√°gina actual
                                });
                            })
                            .catch(function (error) {
                                console.error(error);
                            });

                    }


                    function botoneliminar(idproveedor) {
                        // Utiliza SweetAlert2 para mostrar una ventana modal de confirmaci√≥n
                        Swal.fire({
                            title: '¬øEst√°s seguro?',
                            text: 'Esta acci√≥n eliminar√° al Proveedor con ID ' + idproveedor,
                            icon: 'warning',
                            showCancelButton: true,
                            confirmButtonText: 'S√≠, eliminar',
                            cancelButtonText: 'Cancelar'
                        }).then((result) => {
                            if (result.isConfirmed) {
                                // El usuario confirm√≥ la eliminaci√≥n, realiza la solicitud DELETE con Axios
                                axios.delete(`/admin/proveedores/${idproveedor}`)
                                    .then((response) => {
                                        // Si la solicitud DELETE se completa con √©xito, puedes manejar la respuesta aqu√≠
                                        console.log('Proveedor eliminado con √©xito');
                                        // Puedes realizar acciones adicionales, como redirigir a otra p√°gina o actualizar la interfaz de usuario
                                        window.location.reload();
                                    })
                                    .catch((error) => {
                                        // Si hay un error en la solicitud DELETE, puedes manejarlo aqu√≠
                                        console.error('Error al eliminar al Proveedor:', error);
                                        // Puedes mostrar un mensaje de error o realizar otras acciones seg√∫n sea necesario
                                    });
                            }
                        });
                    }

                    function botonveract(idproveedor) {
                        // Utiliza SweetAlert2 para mostrar una ventana modal de confirmaci√≥n
                        Swal.fire({
                            title: '¬øEst√°s seguro?',
                            text: 'Esta acci√≥n restablecera al Proveedor con ID ' + idproveedor,
                            icon: 'warning',
                            showCancelButton: true,
                            confirmButtonText: 'S√≠, restablecer',
                            cancelButtonText: 'Cancelar'
                        }).then((result) => {
                            if (result.isConfirmed) {
                                // El usuario confirm√≥ la eliminaci√≥n, realiza la solicitud DELETE con Axios
                                axios.delete(`/admin/borraproveedor/${idproveedor}`)
                                    .then((response) => {
                                        // Si la solicitud DELETE se completa con √©xito, puedes manejar la respuesta aqu√≠
                                        console.log('Proveedor restablecido con √©xito');
                                        // Puedes realizar acciones adicionales, como redirigir a otra p√°gina o actualizar la interfaz de usuario
                                        window.location.reload();
                                    })
                                    .catch((error) => {
                                        // Si hay un error en la solicitud DELETE, puedes manejarlo aqu√≠
                                        console.error('Error al restablecer Proveedor:', error);
                                        // Puedes mostrar un mensaje de error o realizar otras acciones seg√∫n sea necesario
                                    });
                            }
                        });
                    }


                    // CONSULTA A LA API DE SUNAT
                    const botonproveedor = document.getElementById("buscarprove");

                    // Asignar el evento click al bot√≥n
                    botonproveedor.addEventListener("click", obtenerDatosProveedor);

                    async function obtenerDatosProveedor() {
                        const rucProveedor = document.getElementById("ruc").value;
                        const nombreInput = document.getElementById("nombre_empresa");
                        const direccionInput = document.getElementById('direccion');
                        const ciudadInput = document.getElementById('ciudad');

                        if (rucProveedor.length === 11) {
                            const apiUrl = `https://dniruc.apisperu.com/api/v1/ruc/${rucProveedor}?token=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJlbWFpbCI6ImhsZmVybmFuZGV6ckBhbHVtbm8udW5zbS5lZHUucGUifQ.lJpdzo238Z3HdZVTfwqp46EAT08Q_V9MLCiTBUy__O4`;

                            try {
                                const response = await fetch(apiUrl);
                                const data = await response.json();
                                console.log(data);
                                const nombreProveedorxml = data.razonSocial;
                                const direccionxml = data.direccion;
                                const distritoxml = data.distrito;

                                nombreInput.value = nombreProveedorxml;
                                direccionInput.value = direccionxml;
                                ciudadInput.value = distritoxml;

                                console.log(data);
                            } catch (error) {
                                console.log("Error:", error);
                            }
                        } else if (rucProveedor.length <= 10) {
                            nombreInput.value = "";
                            direccionInput.value = "";
                            ciudadInput.value = "";
                        }
                    }

                    //PARA BUSCAR EN MI TABLA
                    // Obtener referencia al campo de b√∫squeda
                    const campoBusqueda = document.getElementById('busqueda');

                    // Obtener todas las filas de la tabla
                    const filasTabla = document.querySelectorAll('tbody tr');

                    // Agregar un evento de entrada al campo de b√∫squeda
                    campoBusqueda.addEventListener('input', function () {
                        const consulta = campoBusqueda.value.toLowerCase();

                        // Iterar a trav√©s de las filas de la tabla y mostrar/ocultar seg√∫n la consulta
                        filasTabla.forEach(function (fila) {
                            const contenidoFila = fila.textContent.toLowerCase();
                            if (contenidoFila.includes(consulta)) {
                                fila.style.display = 'table-row';
                            } else {
                                fila.style.display = 'none';
                            }
                        });
                    });
                    //PARA LIMITAR A 7 NUMEROS
                    function limitarADigitos(input, maxDigitos) {
                        // Elimina cualquier car√°cter no num√©rico
                        input.value = input.value.replace(/[^0-9]/g, '');

                        // Limita la longitud a maxDigitos
                        if (input.value.length > maxDigitos) {
                            input.value = input.value.slice(0, maxDigitos);
                        }
                    }

                    //PARA HACER QUE SEA PURA MAYUSCULA Y SIN NUMEROS
                    function validarYConvertir(input) {
                        // Expresi√≥n regular que acepta solo letras y espacios
                        var regex = /^[A-Za-z\s]+$/;

                        if (!regex.test(input.value)) {
                            // Si el valor no cumple con la expresi√≥n regular, elimina los caracteres no permitidos
                            input.value = input.value.replace(/[^A-Za-z\s]+/, '');
                        }

                        // Convierte a may√∫sculas
                        input.value = input.value.toUpperCase();
                    }


                    //PARA ESCRIBIR PURA MAYUSCULA
                    //function convertirAMayusculas(input) {
                       // input.value = input.value.toUpperCase();
                    //}


                </script>
